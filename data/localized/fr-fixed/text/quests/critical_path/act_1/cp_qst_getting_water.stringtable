    <Entry>
      <ID>6</ID>
      <DefaultText>Collectez des baies pour concocter un remède.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>Attaquez la caravane.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>8</ID>
