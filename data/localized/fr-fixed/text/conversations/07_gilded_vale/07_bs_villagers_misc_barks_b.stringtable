<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\07_gilded_vale\07_bs_villagers_misc_barks_b</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>8</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Et là je lui ai dit - ce n'est pas le pistolet !"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Ha !"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Cette bière vous semble étrange ?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Vous pensez qu'elle dira 'oui' ?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Elle est assez folle pour rester avec vous, pourquoi ne pas vous épouser tant qu'elle y est ?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"Je suppose qu'ils ont de la meilleure chique de goldrot en ville."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>"En ville, les êtres civilisés sont accros au svef, pas au goldrot."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>8</ID>
      <DefaultText>"... le Dyrford, alors ?"</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>