    <Entry>
      <ID>10</ID>
      <DefaultText>Vous vous écartez du terrier et vous vous retrouvez dans les mines de Stalwart.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>11</ID>
      <DefaultText>Vous suivez vos pas et regagnez la sortie.</DefaultText>
      <FemaleText />
