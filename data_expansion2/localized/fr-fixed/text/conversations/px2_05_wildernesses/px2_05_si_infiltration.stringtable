      <ID>48</ID>
      <DefaultText>[Slot 5].</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>49</ID>
      <DefaultText>[Essayez autre chose.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>50</ID>
      <DefaultText>[Faites-le vous-même.]</DefaultText>
      <FemaleText />
